{
  "canisters": {
    "product": {
      "type": "motoko",
      "main": "backend/Product.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "backend": {
      "type": "motoko",
      "main": "backend/Backend.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "token": {
      "type": "motoko",
      "main": "backend/token/main.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "internet-identity": {
      "type": "pull",
      "id": "rdmx6-jaaaa-aaaaa-aaadq-cai"
    },
    "frontend": {
      "dependencies": ["backend", "token"],
      "type": "assets",
      "source": ["dist/"]
    }
  },
  
  "defaults": {
    "build": {
      "packtool": "npm run --silent sources"
    }
  },
  "output_env_file": ".env",
  "version": 2
}